<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jbos.app.sys.mapper.MenuMapper">

    <!--查询用户菜单权限列表-->
    <select id="getUserMenuList"  parameterType="java.util.Map" resultType="com.jbos.app.sys.pojo.Menu">
        SELECT ID,PARENT_ID,MENU_CODE,MENU_NAME,MENU_TYPE,MENU_URL FROM JBOS_T_MENU
        WHERE IS_VALID=1 AND PARENT_ID=#{parentId}
        ORDER BY ORDER_NO
    </select>
</mapper>
